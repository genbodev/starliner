{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "no-console": "warn",
    "no-debugger": "warn",
    "no-alert": "warn"
  },
  "extends": "eslint:recommended"
}